<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.toddbray.basketballstats">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewPlayer">
        </activity>
        <activity android:name=".NewGame">
        </activity>
        <activity android:name=".ViewPlayer">
        </activity>
        <activity android:name=".ViewSchedule">
        </activity>
        <activity android:name=".GameList">
        </activity>
        <activity android:name=".NewStats">
        </activity>

    </application>

</manifest>